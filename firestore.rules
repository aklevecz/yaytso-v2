rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
    match /CARTONS/{document=**} {
      allow read, write: if true;
    }
    match /YAYTSOS/{document=**} {
      allow read, write: if true;
    }
    match /NFTS/{document=**} {
      allow read, write: if true;
    }
    match /Users/{userId} {
      allow read, write: if request.auth.uid == userId;
    }
  }
}